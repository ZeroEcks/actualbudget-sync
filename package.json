{
  "name": "@tim-smart/actualbudget-sync",
  "version": "0.2.1",
  "description": "CLI for syncing banks with Actual Budget",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "packageManager": "pnpm@9.9.0",
  "repository": {
    "url": "https://github.com/tim-smart/",
    "type": "git"
  },
  "scripts": {
    "codegen": "build-utils prepare-v2",
    "clean": "rimraf rm -rf dist/*",
    "build": "tsup && pnpm copy-package-json",
    "build:ts": "tsup",
    "copy-package-json": "tsx scripts/copy-package-json.ts",
    "check": "tsc --noEmit",
    "changeset-version": "changeset version",
    "changeset-publish": "pnpm build && changeset publish"
  },
  "keywords": [],
  "author": "Tim Smart <hello@timsmart.co>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.8",
    "@effect/cli": "^0.42.3",
    "@effect/language-service": "^0.1.0",
    "@effect/platform": "https://pkg.pr.new/Effect-TS/effect/@effect/platform@45c5ade",
    "@effect/platform-node": "https://pkg.pr.new/Effect-TS/effect/@effect/platform-node@45c5ade",
    "@effect/schema": "^0.72.2",
    "@types/node": "^22.5.4",
    "effect": "^3.7.2",
    "glob": "^11.0.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.2.4",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@actual-app/api": "^6.10.0"
  }
}
